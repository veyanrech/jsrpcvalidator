    "version": "0.2.0",
    "configurations": [
        {
            "type": "go",
            "request": "launch",
            "name": "Launch rpc requests",
            "mode": "auto",
            "program": "${workspaceFolder}/go/main.go",
            
        },
        {
            "type": "node",
            "request": "launch",
            "name": "RPC VALIDATION SERVER",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node/server.js",
            "runtimeExecutable": "/opt/homebrew/bin/node"
        }
    ]
}